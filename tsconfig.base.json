{
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": ".",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "module": "esnext",
    "lib": ["es2017", "dom"],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@studying-nx/another-hello": ["apps/another-hello/src/index.ts"],
      "@studying-nx/hello-nx": ["apps/hello-nx/src/index.ts"],
      "@studying-nx/helpers/date": ["libs/helpers/date/src/index.ts"],
      "@studying-nx/my-ds": ["libs/my-ds/src/index.ts"],
      "@studying-nx/packages/another-module": [
        "libs/packages/another-module/src/index.ts"
      ],
      "@studying-nx/packages/my-first-module": [
        "libs/packages/my-first-module/src/index.ts"
      ],
      "@studying-nx/packages/third-module": [
        "libs/packages/third-module/src/index.ts"
      ]
    }
  },
  "exclude": ["node_modules", "tmp"]
}
