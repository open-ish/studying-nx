{
  "root": "core/shell",
  "sourceRoot": "core/shell/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/web:webpack",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "compiler": "babel",
        "outputPath": "dist/core/shell",
        "index": "core/shell/src/index.html",
        "baseHref": "/",
        "main": "core/shell/src/main.tsx",
        "polyfills": "core/shell/src/polyfills.ts",
        "tsConfig": "core/shell/tsconfig.app.json",
        "assets": ["core/shell/src/favicon.ico", "core/shell/src/assets"],
        "styles": [],
        "scripts": [],
        "webpackConfig": "@nrwl/react/plugins/webpack"
      },
      "configurations": {
        "production": {
          "fileReplacements": [
            {
              "replace": "core/shell/src/environments/environment.ts",
              "with": "core/shell/src/environments/environment.prod.ts"
            }
          ],
          "optimization": true,
          "outputHashing": "all",
          "sourceMap": false,
          "namedChunks": false,
          "extractLicenses": true,
          "vendorChunk": false
        }
      }
    },
    "serve": {
      "executor": "@nrwl/web:dev-server",
      "options": {
        "buildTarget": "shell:build",
        "hmr": true
      },
      "configurations": {
        "production": {
          "buildTarget": "shell:build:production",
          "hmr": false
        }
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["core/shell/**/*.{ts,tsx,js,jsx}"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/core/shell"],
      "options": {
        "jestConfig": "core/shell/jest.config.js",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
