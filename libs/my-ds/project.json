{
  "root": "libs/my-ds",
  "sourceRoot": "libs/my-ds/src",
  "projectType": "library",
  "tags": ["ds"],
  "targets": {
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/my-ds/**/*.{ts,tsx,js,jsx}"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/my-ds"],
      "options": {
        "jestConfig": "libs/my-ds/jest.config.js",
        "passWithNoTests": true
      }
    }
  }
}
